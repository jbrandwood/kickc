import "print.kc"

void main() {
    print_cls();
    byte r;
    byte[10] cs = { 0, 20, 4, 9, 11, 4 , 13, 8, 4, 6 };
    for( byte a: 0..10 ) {
        byte b = 10-a;
        r = '-'; if(a<b) r='+'; printu(a, '<', b, r);
        r = '-'; if(a<3) r='+'; printu(a, '<', 3, r);
        r = '-'; if(a<cs[a]) r='+'; printu(a, '<', cs[a], r);
        print_ln();
    }

}


void printu(byte a, byte op, byte b, byte res) {
    print_char(' ');
    print_byte(a);
    print_char(op);
    print_byte(b);
    print_char(' ');
    print_char(res);
}
