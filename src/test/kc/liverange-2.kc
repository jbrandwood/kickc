// Test effective live range and register allocation

void main() {
    for(char a: 0..100 ) {
        for( char b: 0..100 ) {
            for( char c: 0..100 ) {
                char ca = c+a;
                print(b, ca);
            }
        }
    }
}

const char* SCREEN  = 0x0400;

void print(char b, char ca) {
    SCREEN[b] = ca;
}