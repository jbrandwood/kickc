import "sinus.kc"
import "print.kc"

void main() {
    word wavelength = 192;
    signed byte[192] sintabb;
    sin8s_gen(sintabb, wavelength);
    signed word[192] sintabw;
    sin16s_gen(sintabw, wavelength);
    print_cls();
    for(byte i: 0..191) {
        signed byte sb = sintabb[i];
        signed word sw = *(sintabw+(word)i<<1);
        signed byte sd = sb-(signed byte)>sw;
        if(sd>=0) {
          print_str(" ");
        }
        print_sbyte(sd);
        print_str("  ");
    }
}
