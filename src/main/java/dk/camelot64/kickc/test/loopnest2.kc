byte* SCREEN = $0400;

main();

void main() {
    byte i=100;
    do {
        byte j=100;
        do {
            nest1();
        } while (--j>0)
    } while (--i>0)
}

void nest1() {
    byte i=100;
    do {
        byte j=100;
        do {
            nest2();
        } while (--j>0)
    } while (--i>0)
}

void nest2() {
    byte i=100;
    do {
        byte j=100;
        do {
            *SCREEN = j;
        } while (--j>0)
    } while (--i>0)
}
