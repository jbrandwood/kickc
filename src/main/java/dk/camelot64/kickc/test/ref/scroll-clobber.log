byte* SCREEN = $0400;
byte* SCROLL = $d016;
byte[] TEXT = "01234567@";

void main() {
    byte* nxt = TEXT;
    byte i=0;
    do {
        byte c = *nxt;
        if(c=='@') {
           nxt = TEXT;
           c = *nxt;
        }
        SCREEN[++i] = c;
        nxt++;
    } while (true);
}

