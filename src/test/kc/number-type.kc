// Tests the number type used for constant expressions

const byte* SCREENB = 0x0400;

void main() {
    // Constant values resolvable to bytes
    byte idx = 0;
    SCREENB[idx++] = 12;
    SCREENB[idx++] = 6+6;
    SCREENB[idx++] = 18-6;
    SCREENB[idx++] = 1812-1800;
    SCREENB[idx++] = 1+2+3+6;
    SCREENB[idx++] = 2*6;
    SCREENB[idx++] = 3<<2;
    SCREENB[idx++] = 24>>1;
    SCREENB[idx++] = 15&28;
    SCREENB[idx++] = 4|8;
    SCREENB[idx++] = 5^9;
    SCREENB[idx++] = (2+2)*(15/5);
    SCREENB[idx++] = (byte)(4096+12);
}


