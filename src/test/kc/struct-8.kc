// Minimal struct - nested struct where a sub-struct is assigned out

struct Point {
    byte x;
    byte y;
};

struct Circle {
    struct Point center;
    byte radius;
};

void main() {
    struct Point p = { 10, 10 };
    struct Circle c = { p, 5 };
    struct Point point = c.center;
    const byte* SCREEN = 0x0400;
    SCREEN[0] = point.x;
    SCREEN[1] = point.y;
    SCREEN[1] = c.radius;
}
