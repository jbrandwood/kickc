
void main() {
  char *maze = 0xa000;
  word width=40;
  word height=25;
  ShowMaze(maze, width, height);
}

const char* SCREEN = 0x0400;

void ShowMaze(const char *maze, word width, word height) {
    char* s = SCREEN;
    char* m = maze;
    for( word i=0;i<width-1; i++)
        for( word j=0;j<width-1; j++)
            *s++ = *m++;
}
