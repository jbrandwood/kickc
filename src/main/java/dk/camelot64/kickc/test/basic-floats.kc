

void main() {

    // Load word register Y,A into FAC (floating point accumulator)
    asm {
        lda #$55
        ldy #$aa
        jsr $b391
    }

    // Load FAC (floating point accumulator) integer part into word register Y,A
    asm {
        jsr $b1aa
        sty $fe
        sta $ff
    }

}