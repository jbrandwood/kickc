
void main() {
    byte[] bs = { 'c', 'm' };   // constant byte array
    byte b = 4;                 // constant byte
    word w = { b, 0 };          // constant inline word
    word w2 = { 1, 1 } + w;     // constant inline word inside expression
    byte* sc = w2;              // implicit cast to (byte*)
    *sc = bs[0];
}

