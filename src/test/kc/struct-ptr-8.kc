// Minimal struct -  variable array access

struct Point {
    byte x;
    byte y;
};

struct Point[2] points;

void main() {
    for( byte i: 0..1) {
        points[i].x = 2+i;
        points[i].y = 3+i;
    }

    const byte* SCREEN = 0x0400;
    byte idx = 0;
    for( byte i: 0..1) {
        SCREEN[idx++] = points[i].x;
        SCREEN[idx++] = points[i].y;
        SCREEN[idx++] = ' ';
    }
}