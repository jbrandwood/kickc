// Minimal struct -  array with 256+ structs

struct Point {
    byte x;
    byte y;
};

struct Point[500] points;

void main() {
    for( word i: 0..499) {
        points[i] = { 2, (byte)i };
    }
    const struct Point* SCREEN = 0x0400;
    for( word i: 0..499) {
        SCREEN[i] = points[i];
    }

}