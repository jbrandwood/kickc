  // Upstart
.pc = $801 "Basic"
:BasicUpstart(main)
.pc = $80d "Program"
  // Global Constants & labels
  .const SIZEOF_STRUCT_POINT = 2
  .const OFFSET_STRUCT_POINT_Y = 1
  .label SCREEN = $400
  .label idx = 2
  // main
main: {
    .label ptr = point
    .label point = 3
    // point = { 1, 2 }
    // [0] *(&(struct Point) main::point) ← memcpy(*(&(const struct Point) $0), struct Point, (const byte) SIZEOF_STRUCT_POINT) -- _deref_pssc1=_deref_pssc2_memcpy_vbuc3 
    ldy #SIZEOF_STRUCT_POINT
  !:
    lda __0-1,y
    sta point-1,y
    dey
    bne !-
    // print(point)
    // [1] (byte) print::p_x#0 ← *((byte*)&(struct Point) main::point) -- vbuyy=_deref_pbuc1 
    ldy.z point
    // [2] (byte) print::p_y#0 ← *((byte*)&(struct Point) main::point+(const byte) OFFSET_STRUCT_POINT_Y) -- vbuxx=_deref_pbuc1 
    ldx point+OFFSET_STRUCT_POINT_Y
    // [3] call print 
    // [8] phi from main to print [phi:main->print]
    // [8] phi (byte) print::p_y#2 = (byte) print::p_y#0 [phi:main->print#0] -- register_copy 
    // [8] phi (byte) idx#12 = (byte) 0 [phi:main->print#1] -- vbuz1=vbuc1 
    lda #0
    sta.z idx
    // [8] phi (byte) print::p_x#2 = (byte) print::p_x#0 [phi:main->print#2] -- register_copy 
    jsr print
    // main::@1
    // print(*ptr)
    // [4] (byte) print::p_x#1 ← *((byte*)(const struct Point*) main::ptr) -- vbuyy=_deref_pbuc1 
    ldy.z ptr
    // [5] (byte) print::p_y#1 ← *((byte*)(const struct Point*) main::ptr+(const byte) OFFSET_STRUCT_POINT_Y) -- vbuxx=_deref_pbuc1 
    ldx ptr+OFFSET_STRUCT_POINT_Y
    // [6] call print 
    // [8] phi from main::@1 to print [phi:main::@1->print]
    // [8] phi (byte) print::p_y#2 = (byte) print::p_y#1 [phi:main::@1->print#0] -- register_copy 
    // [8] phi (byte) idx#12 = (byte) idx#13 [phi:main::@1->print#1] -- register_copy 
    // [8] phi (byte) print::p_x#2 = (byte) print::p_x#1 [phi:main::@1->print#2] -- register_copy 
    jsr print
    // main::@return
    // }
    // [7] return 
    rts
}
  // print
// print(byte register(Y) p_x, byte register(X) p_y)
print: {
    // SCREEN[idx++] = p.x
    // [9] *((const nomodify byte*) SCREEN + (byte) idx#12) ← (byte) print::p_x#2 -- pbuc1_derefidx_vbuz1=vbuyy 
    tya
    ldy.z idx
    sta SCREEN,y
    // SCREEN[idx++] = p.x;
    // [10] (byte) idx#3 ← ++ (byte) idx#12 -- vbuyy=_inc_vbuz1 
    iny
    // SCREEN[idx++] = p.y
    // [11] *((const nomodify byte*) SCREEN + (byte) idx#3) ← (byte) print::p_y#2 -- pbuc1_derefidx_vbuyy=vbuxx 
    txa
    sta SCREEN,y
    // SCREEN[idx++] = p.y;
    // [12] (byte) idx#13 ← ++ (byte) idx#3 -- vbuz1=_inc_vbuyy 
    iny
    sty.z idx
    // print::@return
    // }
    // [13] return 
    rts
}
