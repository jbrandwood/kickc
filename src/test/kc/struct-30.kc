// Minimal struct with MemberUnwind behavior - array member and local initializer

struct Point {
    char x;
    char initials[3];
};

const char* SCREEN = 0x0400;


void main() {
    struct Point point1 = { 2, "jg" };
    SCREEN[0] = point1.x;
    SCREEN[1] = point1.initials[0];
    SCREEN[2] = point1.initials[1];
}