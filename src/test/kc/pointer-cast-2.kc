// Tests casting pointer types to other pointer types

void main() {
    {
    const byte* ub_screen = $400;
    byte ub = 0xff;
    signed byte* sb_ptr = (signed byte*) &ub;
    *sb_ptr = 1;
    *ub_screen = ub;
    }

    {
    const signed byte* sb_screen = $428;
    signed byte sb = (signed byte)0x7f;
    byte* ub_ptr = (byte*) &sb;
    *ub_ptr = 1;
    *sb_screen = sb;
    }

}