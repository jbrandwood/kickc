// Illustrates both break & continue statements in a loop
// Prints a message ending at '@' skipping all spaces
void main() {
    byte* screen = $400;
    byte[] str = "hello brave new world@";
    for( byte i: 0..255) {
    	if(str[i]=='@') break;
    	if(str[i]==' ') continue;
    	*screen++ = str[i];
    }
}
