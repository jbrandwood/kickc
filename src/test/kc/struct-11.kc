// Example of a struct containing an array

import "stdlib"
import "print"

struct Person {
    unsigned long id;
    char[3] initials;
};

struct Person jesper = { 111172, "jg" };
struct Person henry = { 280173, "hg" };

void main() {
    print_person(jesper);
    print_person(henry);
}

void print_person(struct Person person) {
    print_dword_decimal(person.id);
    print_char(' ');
    print_str(person.initials);
    print_ln();
}