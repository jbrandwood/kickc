  __b2:
    // points+i
    // [10] main::$10 = main::i1#2 << 1 -- vbuyy=vbuxx_rol_1 
    txa
    asl
    tay
    // point_i = points+i
    // [11] main::point_i1#0 = points + main::$10 -- pssz1=pssc1_plus_vbuyy 
    tya
    clc
    adc #<points
    sta.z point_i1
    lda #>points
