    // (SCREEN+40)[i] = *((byte*)point_i+OFFS_Y)
    // [13] (main::SCREEN+$28)[main::i1#2] = ((byte*)main::point_i1#0)[OFFS_Y] -- pbuc1_derefidx_vbuxx=pbuz1_derefidx_vbuc2 
    // SCREEN[i] = points[i].x;
    ldy #OFFS_Y
