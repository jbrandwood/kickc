

void main() {
    for( register(X) byte x: 0..100 ) {
        for( byte y: 0..100 ) {
            for( byte a: 0..100 ) {
                register(A) byte val1 = a+x;
                print(y, val1);
            }
        }
    }
}

void print(byte idx, byte val) {
    byte* SCREEN  = $0400;
    SCREEN[idx] = val;
}