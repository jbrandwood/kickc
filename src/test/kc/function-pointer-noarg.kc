// Tests creating pointers to non-args no-return functions

void main() {
    const byte* SCREEN = $400;

    void()* f;
    f = &fn1;
    SCREEN[0] = <(word)f;
    SCREEN[1] = >(word)f;
    f = &fn2;
    SCREEN[2] = <(word)f;
    SCREEN[3] = >(word)f;

}

void fn1() {
    const byte* BORDERCOL = $d020;
    (*BORDERCOL)++;
}

void fn2() {
    const byte* BGCOL = $d021;
    (*BGCOL)++;
}