// Minimal struct - struct return value

struct Point {
    byte x;
    byte y;
};

void main() {
    struct Point q;
    q = point(2,3);

    const byte* SCREEN = 0x0400;
    SCREEN[0] = q.x;
    SCREEN[1] = q.y;
}

struct Point point(byte x, byte y) {
    struct Point p = { x, y };
    return p;
}